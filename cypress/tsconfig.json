{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node"],

    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "isolatedModules": true,

    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": ["**/*.ts"]
}
